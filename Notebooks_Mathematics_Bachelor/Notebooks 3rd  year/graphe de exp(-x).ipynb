    "import numpy as np \n",
    "import matplotlib.pyplot as plt \n",
    "def f(x):\n",
    "    return np.exp(-1*x))\n",
    "a=-5;b=5;\n",
    "xp=np.linspace(a,b,100);\n",
    "plt.plot(xp,f(xp),'r')\n",
    "plt.show()"
   ]
